{
    "name": "pinetco/laravel-vue",
    "description": "Laravel Vue Scaffolding",
    "license": "MIT",
    "authors": [
        {
            "name": "Raviraj Chauhan",
            "email": "raviraj@pinetco.com",
            "homepage": "https://pinetco.com"
        }
    ],
    "homepage": "https://github.com/pinetco/laravel-vue",
    "keywords": ["Laravel", "LaravelVue"],
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Pinetco\\LaravelVue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pinetco\\LaravelVue\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pinetco\\LaravelVue\\LaravelVueServiceProvider"
            ]
        }
    }
}
